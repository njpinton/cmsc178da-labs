    "\n",
    "ax.set_xlabel('GDP (Billion PHP)')\n",
    "ax.set_ylabel('Tourism Arrivals (Thousands)')\n",
    "ax.set_title('GDP vs Tourism Arrivals (by Island Group)', fontweight='bold')\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "corr = df['GDP_Billions'].corr(df['Tourism_Arrivals_K'])\n",
